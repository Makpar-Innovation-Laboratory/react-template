{ "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "s3CrudOps",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::DEPLOYMENT_BUCKET_NAME_GOES_HERE/*"
        },
        {
            "Sid": "s3ObjectQuery",
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::DEPLOYMENT_BUCKET_NAME_GOES_HERE"
        }
    ]
}